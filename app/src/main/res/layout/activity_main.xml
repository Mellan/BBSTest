<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.i_leidian.bbstest.MainActivity">

    <TextView
        android:id="@+id/json_text"
        android:text="Hello"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/kupai" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/send_broad"
        android:text="发送广播"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sendbbs"
        android:text="社区广播"
        android:layout_below="@+id/send_broad"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/send_utilreceiver"
        android:text="UtilReceiver"
        android:layout_below="@+id/getprovider"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/send_H5SupportWebActivity"
        android:text="发送H5SupportWebActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sendbbs"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:text="发送BuiltInBrowserActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/send_BuiltInBrowserActivity"
        android:layout_below="@+id/send_H5SupportWebActivity"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/getprovider"
        android:text="获取provider"
        android:layout_below="@+id/send_BuiltInBrowserActivity"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/kupai"
        android:text="KupaiActivity"
        android:layout_below="@+id/send_utilreceiver"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
